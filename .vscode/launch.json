{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug client.c",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/walker 3d multiplayer/client/client",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/walker 3d multiplayer/client",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build client.c",
      "miDebuggerPath": "/usr/local/bin/gdb"
    }
  ]
}
